      "syntax": "IRG <Xd|SP>, <Xn|SP>{, <Xm>}",
      "encoding": { "format": "Data Processing", "binary_pattern": "10011010110 | Xm | 000100 | Xn | Xd", "hex_opcode": "0x9AC01000" },
      "operands": [{ "name": "Xd", "desc": "Dest Ptr" }, { "name": "Xn", "desc": "Src Ptr" }, { "name": "Xm", "desc": "Exclude Mask" }],
      "extension": "MTE (Memory Tagging)"
    },
    {
