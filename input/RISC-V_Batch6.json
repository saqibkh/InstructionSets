      "operands": [
        { "name": "vd", "desc": "Dest" },
        { "name": "vs2", "desc": "Src 2" },
        { "name": "vs1", "desc": "Src 1" }
      ],
      "pseudocode": "foreach(i < vl): vd[i] = max(vs1[i], vs2[i]);"
    },
    {
      "mnemonic": "VFSGNJ.VV",
