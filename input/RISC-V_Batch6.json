      "architecture": "RISC-V",
      "extension": "V (Vector)",
      "full_name": "Vector Mask Population Count",
      "summary": "Counts set bits in the source mask register.",
      "syntax": "VCPOP.M rd, vs2, vm",
      "encoding": {
        "format": "OPMVX",
        "binary_pattern": "010000 | vm | vs2 | 10000 | 010 | rd | 1010111",
